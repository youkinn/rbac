{
  "name": "nuxt-start",
  "version": "1.0.0",
  "description": "Nuxt.js project",
  "author": "youkinn <03338277@163.com>",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_CONFIG=dev backpack dev",
    "mock": "nodemon -w server -e js,json server/mock/index.js",
    "build:test": "et && cross-env NODE_CONFIG=test nuxt build && backpack build",
    "build:pre": "cross-env NODE_CONFIG=pre nuxt build && backpack build",
    "build:prod": "cross-env NODE_CONFIG=prod nuxt build && backpack build",
    "start:test": "cross-env NODE_CONFIG=test NODE_ENV=production node build/main.js",
    "start:pre": "cross-env NODE_CONFIG=pre NODE_ENV=production node build/main.js",
    "start:prod": "cross-env NODE_CONFIG=prod NODE_ENV=production node build/main.js",
    "deploy:test": "pm2 startOrReload ecosystem.config.js --env test",
    "deploy:pre": "pm2 startOrReload ecosystem.config.js --env pre",
    "deploy:prod": "pm2 startOrReload ecosystem.config.js --env prod",
    "precommit": "npm run lint",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore ."
  },
  "dependencies": {
    "axios": "^0.16.2",
    "body-parser": "^1.18.1",
    "clipboard": "^1.7.1",
    "cross-env": "^5.0.1",
    "element-ui": "2.1.0",
    "express": "^4.15.3",
    "express-session": "^1.15.5",
    "node-sass": "^4.5.3",
    "nuxt": "1.3.0",
    "source-map-support": "^0.4.15",
    "vue": "2.5.13",
    "vue-awesome-swiper": "^2.6.7",
    "vue-quill-editor": "^3.0.5",
    "vue-server-renderer": "2.5.2",
    "vue-template-compiler": "^2.5.13"
  },
  "devDependencies": {
    "@nuxtjs/proxy": "^1.1.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-component": "^0.10.0",
    "backpack-core": "^0.4.1",
    "element-theme": "^2.0.0",
    "element-theme-chalk": "2.0.1",
    "eslint": "^4.3.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.1.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "express-mockjs": "^0.2.1",
    "morgan": "1.8.2",
    "node-sass": "^4.5.3",
    "prettier-stylelint": "^0.4.2",
    "sass-loader": "^6.0.6",
    "stylelint": "^8.4.0",
    "stylelint-config-ydj": "^0.0.1"
  }
}

/**
 * 分页获取文章列表
 *
 * @url /getArticleListByPage
 *
 * 参数说明：
 * pageIndex: 当前页码
 * pageSize:  每页页条数
 *
 */
{
  "code": 0,
  "total": 201,
  "data|10": [
    {
      "_id|+1": 1,
      "title|1-8": "@ctitle",
      "categoryDesc|2-4": "@cword",
      "source|2-8": "@cword",
      "isOriginal|1": [true, false],
      "state|1": [0, 1], // 0:未发布 1：已发布
      "publishTime": "@date('yyyy-MM-dd hh:mm:ss')"
    }
  ]
}
